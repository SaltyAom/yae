{
    "module": {
        "type": "commonjs"
    },
    "exclude": ["(.*).(spec|test).(j|t)s$"],
    "jsc": {
        "target": "es2019",
        "parser": {
            "syntax": "typescript"
        },
        "minify": {
            "mangle": true,
            "compress": true
        }
    },
    "env": {
        "targets": {
            "node": 14
        }
    }
}
